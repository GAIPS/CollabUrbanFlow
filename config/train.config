####################################################
################## Train parameters ################
####################################################
[train_args]
network = grid
# experiment_time = 648000
# experiment_time = 432000
experiment_time = 360000
experiment_save_agent_interval = 3600
tls_type = rl
demand_type = constant
demand_mode = step
epsilon_init = 0.9
epsilon_final = 0.01
epsilon_schedule_timesteps = 359000


####################################################
################ MDP parameters ####################
####################################################
[mdp_args]
feature = delay
action_schema = set
phases_filter = [0, 1, 2, 3]
# use roadlinks as feature-dim or use phase
use_lanes = False

####################################################
################ ENV parameters ####################
####################################################
[env_args]
yellow = 0
min_green = 10
max_green = 90

####################################################
################ Agent's parameters ################
####################################################
[agent_type]
agent_type = GAT
